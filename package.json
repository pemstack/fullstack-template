{
  "name": "project-tracker",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.0",
    "@types/history": "^4.7.2",
    "@types/jest": "24.0.15",
    "@types/node": "12.0.10",
    "@types/react": "16.8.22",
    "@types/react-dom": "16.8.4",
    "classnames": "^2.2.6",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "history": "^4.9.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1",
    "typescript": "3.5.2"
  },
  "scripts": {
    "start:server": "cd server && ts-node-dev --no-notify --respawn --transpileOnly -r tsconfig-paths/register ./server.ts && wait-on tcp:4000",
    "start:app": "react-scripts start",
    "start": "concurrently --kill-others \"npm run start:app\" \"npm run start:server\"",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "source-map-explorer": "^2.0.1",
    "ts-node-dev": "^1.0.0-pre.40",
    "tsconfig-paths": "^3.8.0",
    "tslint": "^5.18.0",
    "tslint-react": "^4.0.0",
    "wait-on": "^3.2.0"
  },
  "proxy": "http://localhost:4000"
}
